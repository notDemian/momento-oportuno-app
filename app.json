{
  "expo": {
    "name": "El momento oportuno",
    "slug": "momento-oportuno",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ff012b"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "config": {
        "googleMapsApiKey": "AIzaSyD7_x-IaDtLZle9C37GEnizuCkEOSnkkBg"
      },
      "infoPlist": {
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app use location service to show user's position on map"
      },
      "supportsTablet": true,
      "buildNumber": "1",
      "bundleIdentifier": "com.momento.oportuno"
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyD7_x-IaDtLZle9C37GEnizuCkEOSnkkBg"
        }
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ff012b"
      },
      "package": "com.momento.oportuno"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Food Star to use your location."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "owner": "vdemian"
  }
}
